{
    "name": "devmachine/form-bundle",
    "type": "symfony-bundle",
    "description": "Set of Symfony form types.",
    "keywords": [ "form", "bootstrap", "typeahead" ],
    "homepage": "https://github.com/lakiboy/devmachine-form-bundle",
    "license": "MIT",
    "authors": [
        { "name": "Dmitri Lakachauskis", "email": "lakiboy83@gmail.com" }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/framework-bundle": "~2.8 || ~3.0",
        "symfony/form": "~2.8 || ~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "symfony/twig-bridge": "~2.8 || ~3.0",
        "matthiasnoback/symfony-config-test": "~1.0",
        "matthiasnoback/symfony-dependency-injection-test": "^0.7",
        "satooshi/php-coveralls": "~0.6"
    },
    "autoload": {
        "psr-4": { "Devmachine\\Bundle\\FormBundle\\": "" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
