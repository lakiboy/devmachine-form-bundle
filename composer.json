{
    "name": "devmachine/form-bundle",
    "type": "symfony-bundle",
    "description": "Symfony form extensions.",
    "keywords": [ "form" ],
    "homepage": "https://github.com/dev-machine/DevmachineFormBundle",
    "license": "MIT",
    "authors": [
        { "name": "Dmitri Lakachauskis", "email": "dmitri@devmachine.net" }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/framework-bundle": "~2.3",
        "symfony/form": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "symfony/twig-bridge": "~2.3",
        "matthiasnoback/symfony-config-test": "~1.0",
        "matthiasnoback/symfony-dependency-injection-test": "~0.7",
        "satooshi/php-coveralls": "~0.6"
    },
    "suggest": {
        "genemu/form-bundle": "*"
    },
    "autoload": {
        "psr-4": { "Devmachine\\FormBundle\\": "" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
